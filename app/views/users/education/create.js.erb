// Close the loading lightbox
closeLightBoxLoading();

<% if @education.errors %>
  <% @education.errors.full_messages.each do |message| %>
    displayGrowlMessages("error","Error!", "<%= message %>", "topRight");
  <% end %>
<% else %>
    displayGrowlMessages("success","Success!", "Education Details has been saved", "topRight");
<% end %>

$('#DIV_EDUCATION_DETAILS').html("<%= escape_javascript(render(:partial=>"/users/dashboard/education")) %>");
