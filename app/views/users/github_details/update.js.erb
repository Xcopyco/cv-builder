// Close the loading lightbox
closeLightBoxLoading();

<% if @git_details.errors.any? %>
  <% @git_details.errors.full_messages.each do |message| %>
    displayGrowlMessages("error","Error!", "<%= message %>", "topRight");
  <% end %>
<% else %>
    displayGrowlMessages("success","Success!", "Github Details has been saved", "top");
<% end %>

$('#DIV_GITHUB_DETAILS').html("<%= escape_javascript(render(:partial=>"/users/dashboard/education")) %>");